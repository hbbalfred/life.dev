# switch input method
cmd + alt - space : /usr/local/bin/im-select com.apple.keylayout.ABC
cmd + alt - h : /usr/local/bin/im-select com.apple.keylayout.ABC && /usr/local/bin/im-select com.apple.inputmethod.SCIM.ITABC
cmd + alt - j : /usr/local/bin/im-select com.apple.keylayout.ABC && /usr/local/bin/im-select com.apple.inputmethod.Kotoeri.RomajiTyping.Japanese
